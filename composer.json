{
    "name": "weijiajia/ip-address",
    "description": "IP address information query system, integrating multiple IP geographic location services",
    "type": "library",
    "keywords": [
        "ip address",
        "ip info"
    ],
    "homepage": "https://github.com/jackchang1025/ip-address",
    "license": "MIT",
    "authors": [
        {
            "name": "weijiajia",
            "email": "shadowmatthew1025@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-mbstring": "*",
        "psr/log": "^3.0",
        "saloonphp/saloon": "^3.10",
        "illuminate/support": "^10.0 || ^11.0 || ^12.0",
        "weijiajia/saloonphp-http-proxy-plugin": "dev-main",
        "weijiajia/saloonphp-logs-plugin": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Weijiajia\\IpAddress\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Weijiajia\\IpAddress\\IpAddressServiceProvider"
            ]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Weijiajia\\IpAddress\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "pestphp/pest": "^3.2"
    }
}
